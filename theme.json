{
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#404040",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#003c71",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#fff200",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#9e9828",
					"name": "Hover",
					"slug": "hover"
				}
			]
		},
		"layout": {
			"contentSize": "15rem",
			"wideSize": "15rem"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fluid": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1.13rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "2rem",
					"slug": "x-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"shadow": "none"
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"bottom": "0.25em",
						"left": "0em",
						"right": "0em",
						"top": "0.75em"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"blocks": {
				"elements": {
					"core/pullquote": {
						"typography": {
							"fontSize": "2.25rem"
						},
						"elements": {
							"cite": {
								"typography": {
									"fontSize": "max( 50%, 1.5rem )"
								}
							}
						}
					}
				}
			}
		}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--accent)",
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--hover)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0em",
				"left": "var(--wp--preset--spacing--40)",
				"right": "var(--wp--preset--spacing--40)",
				"top": "0em"
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "sidebar",
			"title": "Sidebar"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json"
}